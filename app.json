{
  "expo": {
    "name": "passkey-playground",
    "slug": "passkey-playground",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.passkey.playground",
      "supportsTablet": true,
      "associatedDomains": [
        "appLinks:expo.dev",
        "webCredentials:expo.io"
      ]
    },
    "android": {
      "package": "com.passkey.playground",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
